{
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "imports": {
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.4",
    "@dsnp/parquetjs": "npm:@dsnp/parquetjs@^1.8.7",
    "@netlify/functions": "npm:@netlify/functions@^4.1.8",
    "@reduxjs/toolkit": "npm:@reduxjs/toolkit@^2.8.2",
    "@supabase/supabase-js": "npm:@supabase/supabase-js@^2.50.2",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.11",
    "@types/react": "npm:@types/react@^19.0.10",
    "@types/react-dom": "npm:@types/react-dom@^19.0.4",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "apache-arrow": "npm:apache-arrow@^20.0.0",
    "autoprefixer": "npm:autoprefixer@^10.4.21",
    "hyparquet": "npm:hyparquet@^1.17.0",
    "leaflet": "npm:leaflet@^1.9.4",
    "papaparse": "npm:papaparse@^5.5.3",
    "parquetjs": "npm:parquetjs@^0.11.2",
    "postcss": "npm:postcss@^8.5.6",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "react-leaflet": "npm:react-leaflet@^5.0.0",
    "react-redux": "npm:react-redux@^9.2.0",
    "tailwindcss": "npm:tailwindcss@^4.1.11",
    "vite": "npm:vite@^6.1.1",
    "vite-plugin-node-polyfills": "npm:vite-plugin-node-polyfills@^0.23.0",
    "xml2js": "npm:xml2js@^0.6.2"
  }
}
